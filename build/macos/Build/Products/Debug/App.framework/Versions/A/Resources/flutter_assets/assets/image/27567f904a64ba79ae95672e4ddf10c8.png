Container"},"$/travel/article/world-50-best-desserts/index.html.regions.regionContents.1.zones.0.zoneContents.218":{"type":"string","stringContents":{"type":"json","json":{"formattedText":["<h3>Linzer Torte, Austria</h3>"],"id":"paragraph_440F10B5-FF33-896D-22D0-5C2C3B6A77D0"}},"__typename":"PALStringContainer"},"$/travel/article/world-50-best-desserts/index.html.regions.regionContents.1.zones.0.zoneContents.219":{"adSection":null,"autoStartVideo":null,"isVr":null,"type":"element","contentType":"image","title":null,"url":null,"elementContents":{"type":"json","json":{"caption":"","imageAlt":"","imageUrl":"//cdn.cnn.com/cnnnext/dam/assets/181127112145-29-50-sweets-travel-linzer-torte-large-169.jpg","label":"","galleryTitle":"","head":"","source":"Shutterstock","photographer":"Shutterstock","cuts":{"mini":{"width":220,"type":"jpg","uri":"//cdn.cnn.com/cnnnext/dam/assets/181127112145-29-50-sweets-travel-linzer-torte-small-169.jpg","height":124},"xsmall":{"width":307,"type":"jpg","uri":"//cdn.cnn.com/cnnnext/dam/assets/181127112145-29-50-sweets-travel-linzer-torte-medium-plus-169.jpg","height":173},"small":{"width":460,"type":"jpg","uri":"//cdn.cnn.com/cnnnext/dam/assets/181127112145-29-50-sweets-travel-linzer-torte-large-169.jpg","height":259},"medium":{"width":780,"type":"jpg","uri":"//cdn.cnn.com/cnnnext/dam/assets/181127112145-29-50-sweets-travel-linzer-torte-exlarge-169.jpg","height":438},"large":{"width":1100,"type":"jpg","uri":"//cdn.cnn.com/cnnnext/dam/assets/181127112145-29-50-sweets-travel-linzer-torte-super-169.jpg","height":619},"full16x9":{"width":1600,"type":"jpg","uri":"//cdn.cnn.com/cnnnext/dam/assets/181127112145-29-50-sweets-travel-linzer-torte-full-169.jpg","height":900},"mini1x1":{"width":120,"type":"jpg","uri":"//cdn.cnn.com/cnnnext/dam/assets/181127112145-29-50-sweets-travel-linzer-torte-small-11.jpg","height":120}},"responsiveImage":true,"originalImageUrl":"//cdn.cnn.com/cnnnext/dam/assets/181127112145-29-50-sweets-travel-linzer-torte.jpg"}},"__typ